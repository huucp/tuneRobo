<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d" x:Class="TuneRoboWPF.Views.MotionTitleItem">
	

	<Grid x:Name="LayoutRoot"  Height="27">
		<Rectangle Fill="{Binding RectangleFillColor}" Stroke="Black" StrokeThickness="0"/>
        <TextBlock x:Name="MotionTitle"  HorizontalAlignment="Left" Height="15" Margin="32,6,0,0" TextWrapping="Wrap" Text="{Binding Title}" VerticalAlignment="Top" Width="211"/>
	</Grid>
</UserControl>
